@model Unit
<link rel="stylesheet" href="~/css/adminPages.css" asp-append-version="true" />

<div class="container border border-2 border-secondary p-3 mt-4">
    <form method="post">
		<div class="row pb-2">
			<h2 class="text-dark"> Delete Unit </h2>
			<hr />
		</div>
	
		<div class="row">
			<div class="my-1 col-6">
				<label asp-for ="Name"></label>
				<input asp-for="Name" disabled class="form-control"/>
			
			</div>
			<div class=" mt-3 col-6 text-end">
				<button type = "submit" class="btn btn-danger" style = "width:150px"> Delete </button>
				<a asp-controller="Unit" asp-action="Index" class="btn btn-info" style = "width:150px">Back to List</a>
			</div>
		</div>
		
    </form>
</div>

